#!/bin/sh

$ cat whoson
date
echo "Users Currently Logged In"
who

do_commit() {
    cmd="git commit -a -m\"Update\""
    echo $cmd
    git add .;
    git commit -a -m"Update"
    git push origin master;
}

while [ $# -gt 0 ]
do
  case $1 in
    -commit |-u) shift; log=$1; do_commit; exit 0;;
  esac
  shift
done
